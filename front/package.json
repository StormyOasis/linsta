{
  "name": "linsta_front",
  "version": "1.0.0",
  "description": "Lauren's Instagram Clone",
  "main": "dist/server.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "tscheck": "tsc",
    "lint": "eslint './src/**/*'",
    "lint:fix": "eslint './src/**/*' --fix",    
    "report": "npm run build && webpack-bundle-analyzer dist/stats.json",
    "clean": "rimraf dist && rimraf build",
    "build": "npm run clean && cross-env NODE_ENV=production npm run client:prod && npm run server:prod",
    "build:dev": "npm run clean && cross-env NODE_ENV=development npm run client:dev && npm run server:dev",
    "client:dev": "webpack --config webpack.client.js --mode=development",
    "server:dev": "webpack --config webpack.server.js --mode=development",
    "client:prod": "webpack --config webpack.client.js --mode=production",
    "server:prod": "webpack --config webpack.server.js --mode=production",
    "start:dev": "npm run build:dev && cross-env PORT=3000 node ./dist/server.js",
    "start": "npm run build && cross-env PORT=3000 node ./dist/server.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/StormyOasis/linsta.git"
  },
  "author": "Lauren Boydstun",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/StormyOasis/linsta/issues"
  },
  "homepage": "https://github.com/StormyOasis/linsta#readme",
  "dependencies": {
    "@koa/cors": "^5.0.0",
    "@koa/router": "^12.0.1",
    "@reduxjs/toolkit": "^2.2.5",
    "compress": "^0.99.0",
    "compression": "^1.7.4",
    "koa": "^2.15.3",
    "koa-static": "^5.0.0",
    "path": "^0.12.7",
    "prop-types": "^15.8.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-error-boundary": "^4.0.13",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.23.1",
    "redux": "^5.0.1",
    "redux-store": "^0.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.7",
    "@babel/core": "^7.24.7",
    "@babel/eslint-parser": "^7.24.7",
    "@babel/plugin-transform-arrow-functions": "^7.24.7",
    "@babel/plugin-transform-class-properties": "^7.24.7",
    "@babel/plugin-transform-runtime": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "7.24.7",
    "@types/compression": "^1.7.5",
    "@types/koa": "^2.15.0",
    "@types/koa__router": "^12.0.4",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@types/styled-components": "^5.1.34",
    "@typescript-eslint/eslint-plugin": "7.13.1",
    "@typescript-eslint/parser": "7.13.1",
    "babel-loader": "8.1.0",
    "babel-plugin-styled-components": "^2.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "copy-webpack-plugin": "^12.0.2",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-webpack-plugin": "^4.2.0",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.6.0",
    "mini-css-extract-plugin": "^2.9.0",
    "nodemon": "^3.1.4",
