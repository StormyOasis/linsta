{
  "name": "linsta_back",
  "version": "1.0.0",
  "description": "Backend for Lauren's Instagram Clone",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test jest --coverage --config=jest.unit.config.js --detectOpenHandles",
    "test:integ": "NODE_ENV=integ jest --config=jest.integ.config.js",
    "start": "NODE_EXTRA_CA_CERTS='/usr/share/es/certs/ca.crt' tsc -p tsconfig.json && node build/index.js",
    "start:prod": "NODE_ENV=production NODE_EXTRA_CA_CERTS='/usr/share/es/certs/ca.crt' tsc -p tsconfig.json && NODE_ENV=production node build/index.js",
    "clean": "rm -rf ./dist/* ./build/* ./package/* ./coverage/* ./app/* ./snapshots/*",
    "build": "tsc -p tsconfig.json --skipLibCheck && npm run copyCert",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "templates": "node scripts/createTemplates.mjs",
    "generateData": "NODE_EXTRA_CA_CERTS='/usr/share/es/certs/ca.crt' node scripts/generateData.mjs",
    "offline": "npm run build && serverless offline",
    "copyCert": "mkdir -p build/certs && cp certs/ca.crt build/certs/ca.crt",
    "deploy": "npm run build && serverless deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/StormyOasis/linsta.git"
  },
  "author": "Lauren Boydstun",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/StormyOasis/linsta/issues"
  },
  "homepage": "https://github.com/StormyOasis/linsta#readme",
  "dependencies": {
    "@aws-sdk/client-location": "^3.645.0",
    "@aws-sdk/client-s3": "^3.645.0",
    "@aws-sdk/client-ses": "^3.623.0",
    "@aws-sdk/client-sns": "^3.623.0",
    "@aws/amazon-location-utilities-auth-helper": "^1.0.7",
    "@elastic/elasticsearch": "^8.18.2",
    "@koa/cors": "^5.0.0",
    "@koa/multer": "^3.0.2",
    "@types/p-retry": "^3.0.0",
    "aws": "^0.0.3-2",
    "aws-lambda": "^1.0.7",
    "axios": "^1.9.0",
    "bcrypt": "^5.1.1",
    "config": "^3.3.12",
    "dotenv": "^16.5.0",
    "express-validator": "^7.1.0",
    "gremlin": "^3.7.3",
    "hot-shots": "^10.2.1",
    "jsonwebtoken": "^9.0.2",
    "lambda-multipart-parser": "^1.0.1",
    "moment": "^2.30.1",
    "multer": "^1.4.5-lts.1",
    "p-retry": "^4.6.2",
    "redis": "^4.7.0",
    "sanitize-html": "^2.13.0",
    "uuid": "^10.0.0",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/plugin-transform-export-namespace-from": "^7.24.7",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "7.24.7",
    "@faker-js/faker": "^9.7.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/aws-lambda": "^8.10.149",
    "@types/bcrypt": "^5.0.2",
    "@types/config": "^3.3.5",
    "@types/gremlin": "^3.6.7",
    "@types/http-status-codes": "^1.2.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.14.8",
    "@types/node-statsd": "^0.1.6",
    "@types/sanitize-html": "^2.13.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "babel-jest": "^29.7.0",
    "dockerize": "^0.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "msw": "^2.3.4",
    "serverless": "^4.15.1",
    "serverless-jest-plugin": "^0.4.0",
    "serverless-offline": "^14.4.0",
    "serverless-prune-plugin": "^2.1.0",
    "ts-jest": "^29.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "undici": "^6.19.4"
  }
}
